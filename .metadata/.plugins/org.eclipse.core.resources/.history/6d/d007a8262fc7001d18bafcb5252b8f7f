package Day3;

public class jumpingStatemnets {

	public static void main(String[] args) {
		
		for(int i=1;i<=10;i++)
		{	
			if(i==5) 
			{
				System.out.println("break");
				break;
			}
			System.out.println(i);
			
		}
		for(int i=1;i<=10;i++)
		{	
			if(i%2 != 0) 
			{
				System.out.println("continue");
				continue;
			}
			System.out.println(i);
		

	}

}
}