package Day8;

class A {
	int a;
	
	void display() {
		System.out.println(a);
	}
}
class B extends A { //B is child class of A
	int b;
	
	void print() {
		
		System.out.println(b);
	}
}

public class InheritanceTypes {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			
		A aobj = new A();
		aobj.a =100;
		aobj.display();
		
		B bobj = new B();
		bobj.b =10023456;
		bobj.print();

		aobj.display();
		
	}

}
