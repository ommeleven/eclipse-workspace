
public class IntroAssignemnts {

	public static void main(String[] args) {
		//program1
		System.out.println("Hello");
		System.out.println("Om Dange");
		
		//second program
		int a = 4;
		int b = 36;
		System.out.println(a+b);
		
		
		// thrid program

		System.out.println(a);
		System.out.println(b);
		int c = a;
		int b = c;
		int a = b;
		System.out.println(a);
		System.out.println(b);
		
		//fourth program
		int x =30;
		int d = 100;
		
		// TODO Auto-generated method stub

	}

}
