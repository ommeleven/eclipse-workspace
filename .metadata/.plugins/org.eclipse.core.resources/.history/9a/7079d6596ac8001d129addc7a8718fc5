		sID = sID;
		Sname = Sname;
	}
	void getStuMarks(int Sub1,int Sub2,int Sub3) {
		Sub1 =  Sub1;
		Sub2 =  Sub2;
		Sub3 = Sub3;
	}
	void totalMarks() {
		
		System.out.println(totalMark);
	}
	}
	public static void main(String[] args) {
		
		
		
			}

