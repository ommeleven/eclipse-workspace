package Day7;

public class thisKeyword {

	int a,b;
	
	void add(int x, int y) {
		a=x;
		b=y;
		
	}
	void display() {
		System.out.println(a);
		System.out.println(b);
		System.out.println(a+b);
	}

	public static void main(String[] args) {
		
		thisKeyword tk = new thisKeyword();
		tk.add(10,20);
		tk.display();

	}

}
