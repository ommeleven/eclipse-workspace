package Day5;

public class stringPresenceinArray {
	public static void main(String[] args) {
		String s[]= {"om","dange","asmi","devidas","jyoti"};
		
		String v = "om";
		
		for(String i:s) {
			if(i.equals(v)) {
				System.out.println("String found");break;
			}
			else {

				System.out.println("String NOT found");
			}
				
				
		}
	
	
	
	}

}
